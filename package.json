{
  "name": "unist-util-find-after",
  "version": "2.0.0",
  "description": "Utility to find a node after another node",
  "license": "MIT",
  "keywords": [
    "unist",
    "node",
    "find",
    "after",
    "util",
    "utility"
  ],
  "repository": "https://github.com/syntax-tree/unist-util-find-after",
  "bugs": "https://github.com/syntax-tree/unist-util-find-after/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "unist-util-is": "^2.0.0"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "esmangle": "^1.0.0",
    "remark": "^6.0.0",
    "remark-cli": "^3.0.0",
    "remark-preset-wooorm": "^1.0.0",
    "nyc": "^10.0.0",
    "tape": "^4.6.2",
    "xo": "^0.17.1"
  },
  "scripts": {
    "build-md": "remark . -foq",
    "build-bundle": "browserify index.js --no-builtins -s unistUtilFindAfter > unist-util-find-after.js",
    "build-mangle": "esmangle unist-util-find-after.js > unist-util-find-after.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "ignore": [
      "unist-util-find-after.js"
    ]
  },
  "remarkConfig": {
    "presets": "wooorm"
  }
}
