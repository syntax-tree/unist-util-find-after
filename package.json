{
  "name": "unist-util-find-after",
  "version": "1.0.1",
  "description": "Utility to find a node after another node",
  "license": "MIT",
  "keywords": [
    "unist",
    "mdast",
    "markdown",
    "retext",
    "natural",
    "language",
    "node",
    "find",
    "after",
    "util",
    "utility"
  ],
  "dependencies": {
    "unist-util-is": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/unist-util-find-after.git"
  },
  "author": {
    "name": "Titus Wormer",
    "email": "tituswormer@gmail.com"
  },
  "files": [
    "index.js",
    "LICENSE"
  ],
  "devDependencies": {
    "browserify": "^11.0.0",
    "esmangle": "^1.0.0",
    "mdast": "^1.0.0",
    "mdast-comment-config": "^1.0.0",
    "mdast-github": "^1.0.0",
    "mdast-lint": "^1.0.0",
    "mdast-slug": "^1.0.0",
    "mdast-validate-links": "^1.0.0",
    "nyc": "^9.0.1",
    "tape": "^4.6.2",
    "xo": "^0.17.1"
  },
  "scripts": {
    "lint": "xo",
    "bundle": "browserify index.js --no-builtins -s unistUtilFindAfter > unist-util-find-after.js",
    "postbundle": "esmangle unist-util-find-after.js > unist-util-find-after.min.js",
    "build-md": "mdast . --quiet",
    "build": "npm run bundle && npm run build-md",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "ignore": [
      "unist-util-find-after.js"
    ]
  }
}
